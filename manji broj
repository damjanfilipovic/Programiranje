#include <cstdlib>
#include <iostream>

using namespace std;

int main(int argc, char *argv[])
{   double a, b;
cout<<"Upisite 2 broja: "<<endl;
cin>>a;
cin>>b;
       if(a<b){
               cout<<"manji broj je: "<<a<<endl;
               }
        else if(a==b){
             cout<<"Brojevi su jednaki: "<<endl;
             }
       else{cout<<"manji broj je: "<<b<<endl;  }     
    system("PAUSE");
    return EXIT_SUCCESS;
}
